{
  "name": "wanttocry-decryptor",
  "version": "2.0.4",
  "description": "Ferramenta gratuita para análise e descriptografia de arquivos .want_to_cry",
  "main": "start.js",
  "scripts": {
    "start": "node start.js",
    "dev": "node start.js",
    "test": "echo \"No tests specified\" && exit 0",
    "version-patch": "node version-bump.js patch --push",
    "version-minor": "node version-bump.js minor --push",
    "version-major": "node version-bump.js major --push",
    "version-patch-local": "node version-bump.js patch",
    "version-minor-local": "node version-bump.js minor",
    "version-major-local": "node version-bump.js major",
    "commit": "npm run version-patch && git add . && git commit -m \"v$(node -p \"require('./package.json').version\"): Atualização automática\""
  },
  "keywords": [
    "decryption",
    "ransomware",
    "security",
    "crypto",
    "wanttocry"
  ],
  "author": "Bruno Michael",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BrunoMichael/decrypt.git"
  },
  "homepage": "https://github.com/BrunoMichael/decrypt#readme"
}